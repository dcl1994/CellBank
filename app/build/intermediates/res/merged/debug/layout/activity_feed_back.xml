<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.example.Activity.FeedBack">
    <RelativeLayout
        android:id="@+id/stemcell_top"
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:paddingBottom="4dip"
        android:paddingTop="3dip"
        android:background="@drawable/title_bk">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/feedback"
            android:textSize="14sp"
            android:textColor="#ffffff"
            android:layout_centerInParent="true"
            />
        <TextView
            android:id="@+id/text_back"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="4.0dip"
            android:drawableLeft="@drawable/share_return"
            android:gravity="center_vertical"
            android:text="@string/return_text"
            android:textColor="@color/white"
            android:textSize="14sp"/>
        <LinearLayout
            android:id="@+id/addgroup"
            android:layout_width="48dp"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:gravity="center"
            android:visibility="gone"
            android:orientation="vertical">
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:clickable="false"
                android:background="@drawable/image_add" />
        </LinearLayout>
    </RelativeLayout>
    <ListView
        android:id="@+id/listview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/stemcell_top"
        android:divider="#00000000"
        android:dividerHeight="0dip">
    </ListView>
</RelativeLayout>
